{
  "name": "unfoldingcx-typescript-boiterplate",
  "homepage": "https://github.com/unfoldingcx/typescript-boiterplate",
  "description": "UnfoldingCX Boiterplate for TypeScript",
  "packageManager": "bun@1.3.2",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "type": "commonjs",
  "main": "index.ts",
  "author": {
    "name": "Unfolding CX",
    "email": "admin@unfolding.cx",
    "url": "https://www.unfolding.cx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unfoldingcx/typescript-boiterplate.git"
  },
  "keywords": [
    "unfoldingcx",
    "typescript",
    "boiterplate",
    "boiterplate-typescript",
    "boiterplate-typescript-unfoldingcx"
  ],
  "bugs": {
    "url": "https://github.com/unfoldingcx/typescript-boiterplate/issues"
  },
  "contributors": [
    {
      "name": "Unfolding CX",
      "email": "admin@unfolding.cx",
      "url": "https://www.unfolding.cx"
    }
  ],
  "scripts": {
    "build:clear": "rm -rf dist",
    "build:production": "bun build --target=node --outdir=dist --sourcemap=linked --minify ./index.ts",
    "build:development": "bun build --target=node --outdir=dist --sourcemap=linked  ./index.ts",
    "build": "bun run build:clear && bun run build:production",
    "restart": "bun run build:development && bun run start",
    "start": "bun run dist/index.js",
    "dev": "nodemon -q --watch src --ext ts --exec 'bun run restart'",
    "test": "jest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts}\""
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.25",
    "@types/bun": "latest",
    "@types/each": "^0.4.33",
    "@types/events": "^3.0.3",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@types/stream-chain": "^2.1.0",
    "@types/stream-json": "^1.7.8",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript-eslint": "^8.46.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@infisical/sdk": "^4.0.6",
    "@openrouter/ai-sdk-provider": "^1.2.1",
    "@toon-format/toon": "^1.0.0",
    "alova": "^3.3.4",
    "async": "^3.2.6",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "each": "^2.7.2",
    "got": "^14.6.3",
    "inquirer": "^12.11.0",
    "json-api-normalize": "^1.1.5",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "npm-check-updates": "^19.1.2",
    "openai": "^6.8.1",
    "stream-chain": "^3.4.0",
    "stream-json": "^1.9.1",
    "util": "^0.12.5",
    "winston": "^3.18.3",
    "zlib": "^1.0.5"
  }
}
